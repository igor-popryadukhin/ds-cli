{
  "name": "deepseek-cli",
  "version": "0.1.0",
  "description": "DeepSeek command line interface",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/cli/index.js",
  "bin": {
    "deepseek": "dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --max-warnings=0 . --fix",
    "format": "prettier --write .",
    "test": "jest --passWithNoTests",
    "prepare": "husky"
  },
  "keywords": [
    "deepseek",
    "cli"
  ],
  "author": "",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "ink": "^6.3.1",
    "openai": "^4.80.1",
    "pino": "^10.0.0",
    "pino-pretty": "^11.3.0",
    "react": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/react": "^19.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "pnpm exec eslint --max-warnings=0 --fix"
    ],
    "*.{json,md}": [
      "pnpm exec prettier --write"
    ]
  }
}
