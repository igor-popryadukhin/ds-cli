{
  "api": {
    "baseUrl": "https://api.deepseek.com",
    "model": "deepseek-chat"
  },
  "logging": {
    "level": "debug"
  },
  "historyDir": "history",
  "sandbox": {
    "mode": "read-only",
    "workspaceRoot": ".",
    "writableRoots": ["./tmp"],
    "allowDeepSeekOnly": true
  },
  "approvals": {
    "policy": "untrusted"
  },
  "exec": {
    "timeoutMs": 120000,
    "envWhitelist": ["PATH", "HOME", "SHELL", "TMPDIR"]
  }
}
