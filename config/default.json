{
  "provider": "deepseek",
  "model": "deepseek-chat",
  "historyDir": "history",
  "logging": {
    "level": "info"
  },
  "sandbox": {
    "mode": "read-only",
    "workspaceRoot": ".",
    "allowDeepSeekOnly": true
  },
  "approvals": {
    "policy": "untrusted"
  },
  "exec": {
    "timeoutMs": 120000,
    "envWhitelist": ["PATH", "HOME", "TMPDIR"]
  }
}
